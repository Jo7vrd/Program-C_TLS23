#include <iostream>
using namespace std;

int main() {
    float data[10], jml = 0;
    float rata=0

    for (int i = 0; i < 10; i++) {
        cout << "Masukkan data ke-" << i + 1 << ": ";
        cin >> data[i];
        jml = jml + data[i];
    }

    rata = jml / 10;

    cout << "Rata-Rata: " << rata << endl;
    return 0;
}
